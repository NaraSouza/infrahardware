module mux_srcB(
	//TODO: Verificar se output está correto
	input wire [3:0] selector,
	input wire [31:0] inputA,//00
	input wire [31:0] inputB,//00 SL2
	input wire [31:0] inputC,//00 SL16
	output wire [31:0] out);
	
	
always
	case(selector)
		3'b000: outputA = inputA;
		3'b001: outputA = 3'd1;
		3'b010: outputA = 3'd4;
		3'b011: outputA = inputB;
		3'b100: outputA = inputC;
endcase
	
endmodule: mux_srcB